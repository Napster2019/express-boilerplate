<% include sidebar.ejs %>

<div class="dt-content">

        <!-- Page Header -->
        <!-- <div class="dt-page__header">
            <h1 class="dt-page__title">Basic Form</h1>
          </div> -->
        <!-- /page header -->
      
      
      
        <!-- Grid -->
        <div class="row">
          <!-- Grid Item -->
          <div class="col-12">
      
            <!-- Card -->
            <div class="dt-card">
      
              <!-- Card Header -->
              <div class="dt-card__header">
      
                <!-- Card Heading -->
                <div class="dt-card__heading">
                  <h3 class="dt-card__title">Upload Files</h3>
                </div>
                <!-- /card heading -->
      
              </div>
              <!-- /card header -->
      
              <!-- Card Body -->
              <div class="dt-card__body">
      
                <!-- Form -->
                <form>
      
      
                  <!-- Form Row -->
                  <div class="form-row input-group">
                    <label class="col-md-2 col-sm-3 col-form-label text-sm-right" for="simple-select">File Tags</label>
                    <div class="col-md-10 col-sm-9">
                      <select class="custom-select" id="inputGroupSelect04">
                        <option selected>Choose File Tags</option>
                        <option value="1">ISO 9001</option>
                        <option value="2">ISO 9000</option>
                        <option value="3">ISO 17700</option>
                      </select>
                    </div>
                  </div>

                  <br />

                  <div class="form-row input-group">
                        <label class="col-md-2 col-sm-3 col-form-label text-sm-right" for="file-field">Attached Files</label>
          
                        <div class="col-md-10 col-sm-9">
                          <div class="custom-file">
                            <input type="file" name="file" class="custom-file-input" id="file-field">
                            <label class="custom-file-label" for="file-field">Choose file...</label>
                          </div>
                        </div>
                      </div>
                </form>
              </div>
            </div>
          </div>
        </div>
</div>

